{
  "name": "avalonia-addfile-mvvm",
  "displayName": "avalonia-addfile-mvvm",
  "description": "help add file in avalonia under mvvm mode",
  "version": "3.1.7",
  "publisher": "machillka",
  "engines": {
    "vscode": ">=1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "avalonia-addfile.create-file",
        "title": "create mvvm file"
      }
    ]
  },
  "files": [
    "templates/**"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
